{
  "datasets": [
    {
      "name": "8ed0b79c",
      "displayName": "Location Data",
      "queryLines": [
        "select \r\n",
        "  latitude,\r\n",
        "  longitude,\r\n",
        "  `GHG emissions mtons CO2e` as emissions\r\n",
        "from emissions.default.emissions_data;"
      ]
    },
    {
      "name": "1af72cc9",
      "displayName": "Emission per Person",
      "queryLines": [
        "select\r\n",
        "  county_state_name,\r\n",
        "  population, \r\n",
        "  (cast(replace(`GHG emissions mtons CO2e`,',','')as double) / nullif(cast(replace(population,',','') as double ),0)) as emissions_per_person\r\n",
        "from emissions.default.emissions_data\r\n",
        "order by emissions_per_person desc;"
      ]
    },
    {
      "name": "5d20b950",
      "displayName": "Emission per State",
      "queryLines": [
        "select\r\n",
        "  state_abbr, \r\n",
        "  sum(cast(replace(`GHG emissions mtons CO2e`,',','')as double)) as emissions_per_sate\r\n",
        "from emissions.default.emissions_data\r\n",
        "group by state_abbr\r\n",
        "order by emissions_per_sate desc\r\n",
        "limit 10;"
      ]
    },
    {
      "name": "780e7605",
      "displayName": "Country Shaming",
      "queryLines": [
        "select\r\n",
        "  county_state_name,\r\n",
        "  population, \r\n",
        "  cast(replace(`GHG emissions mtons CO2e`,',','')as double) as total_emissions\r\n",
        "from emissions.default.emissions_data\r\n",
        "order by total_emissions desc\r\n",
        "limit 10;"
      ]
    }
  ],
